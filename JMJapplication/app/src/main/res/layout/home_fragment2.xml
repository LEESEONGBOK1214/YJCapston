<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".user.MainActivity">

    <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:background="@drawable/edge7"
            android:id="@+id/relativeLayout"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

        <Button
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/bell_button"
                android:width="1dp"
                android:height="1dp"
                android:layout_marginLeft="1dp"
                android:padding="1dp"
                app:strokeWidth="1dp"
                android:background="@drawable/bell"/>
        <TextView
                android:id="@+id/text_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/we_semibold"
                android:textColor="#000"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:text="주소"/>


        <Button
                android:id="@+id/qr_button"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_alignParentRight="true"
                android:background="@drawable/qr"
                android:textColor="@android:color/white"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="7.5dp"
                android:layout_marginBottom="5dp"/>

        <ImageView
                android:layout_width="400dp"
                android:layout_height="30dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/search3"
                android:layout_centerHorizontal="true"
                app:layout_constraintEnd_toEndOf="parent"/>

    </RelativeLayout>


    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/scroll_view1"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout"
            android:layout_marginBottom="1dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:orientation="vertical"
            >

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <androidx.viewpager.widget.ViewPager
                    android:id="@+id/viewPager2"
                    android:layout_width="0dp"
                    android:layout_height="150dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@drawable/main_icon2"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


            <ImageView
                    android:id="@+id/koreanfood"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/rice"
                    tools:ignore="MissingConstraints"
                    android:layout_marginTop="30dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.141"
                    android:layout_marginEnd="10dp"/>
            <ImageView
                    android:id="@+id/japansesfood"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/sushi"
                    tools:ignore="MissingConstraints"
                    android:layout_marginTop="30dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"/>
            <ImageView
                    android:id="@+id/chinesefood"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/chinese"
                    android:layout_marginTop="30dp"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.854"
                    android:layout_marginStart="10dp"/>

            <TextView
                    android:id="@+id/koreantv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="한식"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/koreanfood"
                    android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.150"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="일식"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/japansesfood"
                    android:layout_marginTop="8dp" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:id="@+id/japantv"/>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="중식"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/chinesefood"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.840"
                    android:id="@+id/chinatv"/>

            <ImageView
                    android:id="@+id/meat"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/meat"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.141"
                    android:layout_marginEnd="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/koreantv" android:layout_marginTop="40dp"/>
            <ImageView
                    android:id="@+id/bunsick"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/kimbap"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498" app:layout_constraintTop_toBottomOf="@+id/japantv"
                    android:layout_marginTop="40dp"/>

            <ImageView
                    android:id="@+id/soju"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/soju"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.854"
                    android:layout_marginStart="10dp" app:layout_constraintTop_toBottomOf="@+id/chinatv"
                    android:layout_marginTop="40dp"/>
            <TextView
                    android:id="@+id/meattv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="고기"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/meat"
                    android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.150"/>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="분식"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/bunsick"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:id="@+id/bunsicktv" app:layout_constraintHorizontal_bias="0.498"
                    android:layout_marginTop="8dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="술집"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/soju"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.840"
                    android:id="@+id/sojutv"/>

            <ImageView
                    android:id="@+id/fastfood"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/fastfood"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.141"
                    android:layout_marginEnd="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/meattv" android:layout_marginTop="40dp"/>

            <ImageView
                    android:id="@+id/tang"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/tang"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintTop_toBottomOf="@+id/bunsicktv"
                    android:layout_marginTop="40dp"/>
            <ImageView
                    android:id="@+id/dessert"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/dessert"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.854"
                    android:layout_marginStart="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/sojutv"
                    android:layout_marginTop="40dp"/>

            <TextView
                    android:id="@+id/fastfoodtv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="패스트푸드"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/fastfood"
                    android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.110"/>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="찜.탕"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/tang"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:id="@+id/zzimtang" app:layout_constraintHorizontal_bias="0.498"
                    android:layout_marginTop="8dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="카페.디저트"
                    android:textSize="18sp"
                    android:fontFamily="@font/we_semibold"
                    tools:ignore="MissingConstraints"
                    app:layout_constraintTop_toBottomOf="@+id/dessert"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.905"
                    android:id="@+id/desserttv"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

