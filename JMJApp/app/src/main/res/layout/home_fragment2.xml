            <TextView
                android:id="@+id/bunsicktv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/we_semibold"
                android:text="분식"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/bunsick"
                app:layout_constraintStart_toStartOf="@+id/bunsick"
                app:layout_constraintTop_toBottomOf="@+id/bunsick"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/italytv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/we_semibold"
                android:text="양식"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/italy"
                app:layout_constraintStart_toStartOf="@+id/italy"
                app:layout_constraintTop_toBottomOf="@+id/italy"
                tools:ignore="MissingConstraints" />

            <ImageView
                android:id="@+id/fastfood"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/fastfood"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.141"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/meattv"
                tools:ignore="MissingConstraints" />

            <ImageView
                android:id="@+id/tang"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/tang"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bunsicktv"
                tools:ignore="MissingConstraints" />

            <ImageView
                android:id="@+id/dessert"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/dessert"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.854"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/italytv"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/fastfoodtv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/we_semibold"
                android:text="패스트푸드"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/fastfood"
                app:layout_constraintStart_toStartOf="@+id/fastfood"
                app:layout_constraintTop_toBottomOf="@+id/fastfood"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/zzimtang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/we_semibold"
                android:text="찜.탕"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/tang"
                app:layout_constraintStart_toStartOf="@+id/tang"
                app:layout_constraintTop_toBottomOf="@+id/tang"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/desserttv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/we_semibold"
                android:text="카페.디저트"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@+id/dessert"
                app:layout_constraintStart_toStartOf="@+id/dessert"
                app:layout_constraintTop_toBottomOf="@+id/dessert"
                tools:ignore="MissingConstraints" />

        </androidx.constraintlayout.widget.ConstraintLayout>

